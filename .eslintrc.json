{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "linebreak-style": 0,
        "@typescript-eslint/ban-ts-comment": "off"
    }
}